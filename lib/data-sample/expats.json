    {
      "id": "section-4",
      "order": 4,
      "section_name": "DOCUMENTS",
      "sub_sections": [
        {
          "id": "section-4-subsection-1",
          "order": 1,
          "sub_section_name": "Mandatory",
          "sub_section_info": "",
          "questions": [
            {
              "id": "section-4-subsection-1-question-1",
              "order": 1,
              "question": "Tenancy Agreement / Sales and Purchase / Sub-tenancy Agreement (at least 6 months validity period) and stamped by LHDN.",
              "question_info": "In the event if there is a change of MSC/MD address, re-submission is required\n\nImportant:\n1. Office/business premise is operational during business hours\n2. Company personnel present in office/business premise during business hours\n3. Company is conducting related ICT Sector activities at business premise\n4. Clear demarcation of office space (applicable only to sub-let tenancy agreement)\n5. Company to provide designated office premise pictures (main entrance, reception area, & working space)",
              "type": "Upload",
              "required": true
            },
            {
              "id": "section-4-subsection-1-question-2",
              "order": 2,
              "question": "E-SSM search document (latest 6 months)",
              "question_info": "",
              "type": "Upload",
              "required": true
            },
            {
              "id": "section-4-subsection-1-question-3",
              "order": 3,
              "question": "Phone bill registered under the company name",
              "question_info": "",
              "type": "Upload",
              "required": true
            },
            {
              "id": "section-4-subsection-1-question-4",
              "order": 4,
              "question": "Company profile i.e., business activities, revenue model, clientele, website, Organisation/Group structure and disclose subsidiary(s) if applicable.",
              "question_info": "",
              "type": "Upload",
              "required": true
            },
            {
              "id": "section-4-subsection-1-question-5",
              "order": 5,
              "question": "Registration and Approval Letter from Department of Labour Peninsular Malaysia (JTKSM) **",
              "question_info": "Companies are required to obtain prior approval from Director General, Department of Labour Peninsular Malaysia (JTKSM) before employing Expatriate/Foreign Knowledge Worker (FKW) based on the provisions of Section 60K, Employment Act 1955.\n\nAnnouncement: https://mdec.my/expat/reminder-on-requirement-by-dept-labour-peninsular-malaysia\n\nFor further assistance and detailed information, you may email JTKSM directly at bpajtksm@mohr.gov.my or contact their office at 03-8886 2498/5192/2361.\n\nShould you require more information and assistance, please contact us via email at expatctr@mdec.com.my or contact us at 03-8315 3106/3157",
              "type": "Upload",
              "required": true
            }
          ]
        },
        {
          "id": "section-4-subsection-2",
          "order": 2,
          "sub_section_name": "Optional",
          "sub_section_info": "",
          "questions": [
            {
              "id": "section-4-subsection-2-question-1",
              "order": 1,
              "question": "Latest Master Service Agreement (MSA) / Contract / PO / SOW / LOA (Note: Not applicable for captive services)",
              "question_info": "Information required includes; Client, project name, scope of work, project value, duration, position name, headcounts required, justification to hire FKW, etc.\n\nApplicable only if the company is applying for FKW quota during the subscription stage\n\nExample: Template (Simple Excel) https://view.officeapps.live.com/op/view.aspx?src=https%3A%2F%2Fmdec.my%2Fwp-content%2Fuploads%2F2023%2F06%2FSimplified-FKW-Projection-Template-2023.xlsx&wdOrigin=BROWSELINK",
              "type": "Upload",
              "required": false
            },
            {
              "id": "section-4-subsection-2-question-2",
              "order": 2,
              "question": "Latest certified true copy of company's past years financial audited report and/or management account (year to date) **",
              "question_info": "Applicable for company with more than TWO(2) years in operation",
              "type": "Upload",
              "required": false
            }
          ]
        }
